{
    "presets": [
        "react",
        "es2015"
    ],
    "plugins": [
        "transform-strict-mode",
        "transform-class-properties",
        "transform-object-rest-spread"
    ],
    "env": {
        "development": {
            "plugins": [
                ["react-transform", {
                    "transforms": [{
                        "transform": "react-transform-hmr",
                        "imports": ["react"],
                        "locals": ["module"]
                    }]
                }]
            ]
        }
    }
}
